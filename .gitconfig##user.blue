[user]
	name = Blue Upboard
	email = team@blueecoline.com
[commit]
	gpgsign = false
[pull]
	ff = only
[core]
	autocrlf = input
[credential]
	helper = store
[init]
	defaultBranch = main
[difftool]
    prompt = true
[diff]
    tool = nvim-diff
[difftool "nvim-diff"]
    cmd = "nvim -d \"$LOCAL\" \"$REMOTE\""
